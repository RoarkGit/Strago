    "axios": "^1.4.0",
    "copyfiles": "^2.4.1",
    "cron": "^2.4.1",
    "discord.js": "^14.12.1",
    "dotenv": "^16.3.1",
    "mongoose": "^7.5.0",
    "ts-node": "^10.9.1",
    "winston": "^3.10.0",
    "winston-loki": "^6.0.7",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.7",
